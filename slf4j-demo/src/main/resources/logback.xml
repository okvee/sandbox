<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level \(%marker\) %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="cz.okvee" level="WARN"/>

    <root level="ALL">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
